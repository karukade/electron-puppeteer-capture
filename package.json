{
  "name": "glutton",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main/index.js",
  "scripts": {
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --display-modules --display-used-exports --config ./config//webpack.renderer.config.js --color",
    "dev:main": "cross-env NODE_ENV=development webpack --display-used-exports --config ./config/webpack.main.config.js --color",
    "dev:electron": "electron ./app/main/index.js",
    "start": "yarn dev:main && yarn dev:renderer",
    "build:main": "rm -rf ./app/main && cross-env NODE_ENV=production webpack --config ./config/webpack.main.config.js --color",
    "build:renderer": "rm -rf ./app/renderer && cross-env NODE_ENV=production webpack --config ./config/webpack.renderer.config.js --color --display-modules",
    "build": "yarn build:main && yarn build:renderer",
    "prebuild": "node scripts/beforeBuild",
    "dist": "yarn prebuild && yarn build && electron-builder --config electron-builder.config.js",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "postinstall": "node scripts/prepare"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@karukade/electron-redux-ts": "0.0.1",
    "antd": "^4.4.3",
    "exceljs": "^4.1.0",
    "extract-zip": "^2.0.1",
    "immer": "^7.0.7",
    "puppeteer": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.39.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.1.1",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "yargs": "^15.4.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.23",
    "@types/puppeteer": "^3.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.1",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "archiver": "^4.0.1",
    "cli-progress": "^3.8.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.3.0",
    "image-size": "^0.8.3",
    "jest": "^26.1.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "~4.42.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
